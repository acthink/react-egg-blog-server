{
  "name": "blog-server",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^3.17.5",
    "egg-scripts": "^2.17.0",
    "egg-sequelize": "^6.0.0",
    "egg-validate": "^2.0.2",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^3.6.3",
    "nodemailer": "^6.9.7",
    "sequelize-cli": "^6.6.1"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-blog-server",
    "stop": "egg-scripts stop --title=egg-server-blog-server",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "migrate": "npx sequelize db:migrate",
    "undom": "npx sequelize db:migrate:undo",
    "seed": "npx sequelize db:seed:all",
    "generate": "npx sequelize migration:generate --name"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
